<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sheet1>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Title</sheetTitle>
                    <sheetDescription>Descr.</sheetDescription>
                </TCEforms>
                <type>array</type>
                <el>
                <settings.references>
				<section>1</section>
				<type>array</type>
				<el>
					<item>
						<type>array</type>
						<title>LLL:EXT:fsf_slider/Resources/Private/Language/locallang.xlf:flexform.slide.title</title>
						<el>
                            <settings.headline>
                                <TCEforms>
                                    <label>Überschrift</label>
                                    <config>
                                        <type>input</type>
                                        <eval>trim</eval>
                                        <size>30</size>
                                    </config>
                                </TCEforms>
                            </settings.headline>
                            <settings.text>
                                <TCEforms>
                                    <label>Unterüberschrift</label>
                                    <config>
                                        <type>text</type>
                                        <rows>5</rows>
                                        <cols>30</cols>
                                        <eval>trim</eval>
                                        <enableRichtext>1</enableRichtext>
                                        <richtextConfiguration>default</richtextConfiguration>
                                    </config>
                                </TCEforms>
                            </settings.text>
                            <settings.buttontext1>
                                <TCEforms>
                                    <label>Buttontext</label>
                                    <config>
                                        <type>input</type>
                                        <eval>trim</eval>
                                        <size>30</size>
                                    </config>
                                </TCEforms>
                            </settings.buttontext1>
                            <settings.url1>
                                <TCEforms>
                                    <label>Buttonlink</label>
                                    <config>
                                        <type>input</type>
                                        <size>50</size>
                                        <max>256</max>
                                        <eval>trim</eval>
                                        <renderType>inputLink</renderType>
                                        <softref>typolink</softref>
                                    </config>
                                </TCEforms>
                            </settings.url1>
                            <settings.img1>
                                <TCEforms>
                                    <label>Hintergrund Bild</label>
                                    <config>										
                                        <type>group</type>
                                        <internal_type>file</internal_type>
                                        <allowed>gif, jpg,jpeg,png,csv</allowed>
                                        <max_size>10000</max_size>
                                        <uploadfolder>uploads/tx_fsfzollersimpleheader</uploadfolder>
                                        <maxitems>1</maxitems>
                                        <size>1</size>
                                        <selectedliststyle>Width:180px</selectedliststyle>
                                    </config>
                                </TCEforms>
                            </settings.img1>
                            <settings.video>
                                    <TCEforms>
                                        <label>Video/Animation</label>
                                        <config>
                                            <type>group</type>
                                            <internal_type>file</internal_type>
                                            <allowed>mp4, mov</allowed>
                                            <max_size>10000</max_size>
                                            <uploadfolder>uploads/tx_fsfzollersimpleheader</uploadfolder>
                                            <maxitems>1</maxitems>
                                            <size>1</size>
                                            <selectedliststyle>Width:180px</selectedliststyle>
                                        </config>
                                    </TCEforms>
                            </settings.video>
						</el>
					</item>
				</el>
			</settings.references>
                    
                </el>
            </ROOT>
        </sheet1>
    </sheets>
</T3DataStructure>
